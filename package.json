{
  "name": "nextjs-typescript",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "develop": "nodemon",
    "develop-dev": "APP_ENV=dev yarn develop",
    "develop-qa": "APP_ENV=qa yarn develop",
    "develop-stage": "APP_ENV=stage yarn develop",
    "develop-prod": "APP_ENV=prod yarn develop",
    "now-build": "next build",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build ./src/main build && tsc --project tsconfig.server.json",
    "build-dev": "APP_ENV=dev yarn build",
    "build-qa": "APP_ENV=qa yarn build",
    "build-stage": "APP_ENV=stage yarn build",
    "build-prod": "APP_ENV=prod yarn build",
    "start": "NODE_ENV=production node server/build/server/index.js",
    "lint": "tslint --project ./tsconfig.json --config tslint.json",
    "test:jest": "jest --watch",
    "test:jest:ci": "jest --ci",
    "test:jest:update": "jest --updateSnapshot",
    "test:jest:coverage": "jest --coverage",
    "test:cypress": "start-server-and-test develop-dev http://localhost:3000 cy:open",
    "test:cypress-ci": "start-server-and-test develop-dev http://localhost:3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:install": "cypress install",
    "type-check": "tsc TODO"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@emotion/core": "^10.0.21",
    "cypress-cucumber-preprocessor": "^1.16.1",
    "dotenv": "^8.1.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "i18next": "^17.2.0",
    "mobx": "^5.14.0",
    "mobx-react": "^6.1.3",
    "next": "^9.0.7",
    "next-fonts": "^0.19.0",
    "next-i18next": "^1.2.1",
    "react": "~16.10.1",
    "react-dom": "~16.10.1",
    "react-i18next": "^10.13.1",
    "react-test-renderer": "^16.10.1",
    "ts-node": "^8.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@testing-library/cypress": "^5.0.1",
    "@types/cypress-cucumber-preprocessor": "^1.12.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "~24.0.18",
    "@types/node": "^12.7.8",
    "@types/react": "~16.9.3",
    "@types/react-dom": "~16.9.1",
    "@types/react-test-renderer": "^16.9.0",
    "babel-jest": "~24.9.0",
    "cypress": "~3.4.1",
    "cypress-cucumber-preprocessor": "^1.16.1",
    "jest": "~24.9.0",
    "jest-emotion": "^10.0.17",
    "nodemon": "^1.19.4",
    "react-addons-test-utils": "~15.6.2",
    "start-server-and-test": "^1.10.3",
    "tsify": "^4.0.1",
    "tslint": "~5.20.0",
    "tslint-config-prettier": "~1.18.0",
    "typescript": "~3.6.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  }
}
